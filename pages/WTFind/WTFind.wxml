<swiper class='swiper' indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular='true'>
  <block wx:for="{{banners}}" wx:key = '*this'>
    <swiper-item>
      <image src="{{item.banner_img_url}}" mode='aspectFill' class="slide-image" data-link='{{item.banner_link}}'data-title = '{{item.banner_title}}' catchtap='swiperTap'/>
    </swiper-item>
  </block>
</swiper>

<view class='listContainer'>
<block wx:for = "{{listArray}}" wx:key = "*this">
<findlist-item imageSrc = '{{item.platform_logo}}' title = '{{item.platform_name}}'detail = '{{item.platform_desc}}'link = '{{item.platform_homepage}}'catch:findItemTap = 'findItemTap'></findlist-item>
</block>
</view>
